                    android:padding="3dp"
                    android:text="@string/ttl_spl"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="3"
                    app:layout_constraintLeft_toRightOf="@id/tv_total_spl"
                    app:layout_constraintRight_toLeftOf="@id/tv_total_spl2"
                    app:layout_constraintTop_toTopOf="@id/tv_title_total_spl" />

                <TextView
                    android:id="@+id/tv_total_spl2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="#00796B"
                    android:gravity="center"
                    android:padding="3dp"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="2"
                    app:layout_constraintLeft_toRightOf="@id/tv_title_total_spl2"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@id/tv_title_total_spl" />

                <!--STDR-->
                <TextView
                    android:id="@+id/tv_title_total_stdr"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="3dp"
                    android:background="#38C9A3"
                    android:padding="3dp"
                    android:text="@string/stdr"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="3"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toLeftOf="@id/tv_total_stdr"
                    app:layout_constraintTop_toBottomOf="@id/tv_total_spl2" />

                <TextView
                    android:id="@+id/tv_total_stdr"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="3dp"
                    android:background="#00796B"
                    android:gravity="center"
                    android:padding="3dp"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="2"
                    app:layout_constraintLeft_toRightOf="@id/tv_title_total_stdr"
                    app:layout_constraintRight_toLeftOf="@id/tv_title_total_stdr2"
                    app:layout_constraintTop_toTopOf="@id/tv_title_total_stdr" />

                <TextView
                    android:id="@+id/tv_title_total_stdr2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="3dp"
                    android:background="#38C9A3"
                    android:padding="3dp"
                    android:text="@string/stdr"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="3"
                    app:layout_constraintLeft_toRightOf="@id/tv_total_stdr"
                    app:layout_constraintRight_toLeftOf="@id/tv_total_stdr2"
                    app:layout_constraintTop_toTopOf="@id/tv_title_total_stdr" />

                <TextView
                    android:id="@+id/tv_total_stdr2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="#00796B"
                    android:gravity="center"
                    android:padding="3dp"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="2"
                    app:layout_constraintLeft_toRightOf="@id/tv_title_total_stdr2"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@id/tv_title_total_stdr" />

                <!--'<STD-->
                <TextView
                    android:id="@+id/tv_title_total_std"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="3dp"
                    android:background="#38C9A3"
                    android:padding="3dp"
                    android:text="@string/std"
                    android:textColor="@android:color/white"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintHorizontal_weight="3"
